{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "Webpack configuration and helpers for the AEM.Design compose module",
  "engines": {
    "node": ">= 10.x",
    "yarn": ">= 1.10.x"
  },
  "bin": {
    "aemdesign-compose": "dist/bin/cli-service.js"
  },
  "files": [
    "configs/",
    "dist/",
    "types/*.d.ts",
    ".browserlistrc"
  ],
  "keywords": [
    "aem design",
    "aem",
    "design",
    "webpack-configuration",
    "webpack",
    "compose",
    "node",
    "adobe",
    "webpack-dev-server",
    "front end",
    "front-end",
    "frontend",
    "live-reload",
    "watch",
    "typescript",
    "vue"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "name": "@aem-design/compose-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-design/npm-compose-webpack"
  },
  "bugs": {
    "url": "https://github.com/aem-design/npm-compose-webpack/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run build.compile",
    "build.compile": "cross-env NODE_ENV=production tsc",
    "clean": "rm -rf dist",
    "lint": "eslint '**/*.ts'",
    "postbuild": "chmod +x dist/bin/cli-service.js",
    "postversion": "npm run test && git push && git push --tags",
    "prepack": "npm run build",
    "watch": "tsc --watch"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "version": "1.3.0",
  "dependencies": {
    "@aem-design/compose-support": "1.1.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/register": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/figlet": "^1.2.0",
    "@types/lodash": "^4.14.146",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/mini-css-extract-plugin": "^0.8.0",
    "@types/node": "^12.12.7",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/sass": "^1.16.0",
    "@types/stylelint-webpack-plugin": "^0.1.4",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.39.8",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-config-utils": "^2.3.0",

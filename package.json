{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "Webpack configuration and helpers for the AEM.Design compose module",
  "engines": {
    "node": ">= 10.13.x < 13.x",
    "yarn": ">= 1.10.x < 2.x"
  },
  "bin": {
    "aemdesign-compose": "dist/bin/cli-service.js"
  },
  "files": [
    "configs/",
    "dist/",
    "!dist/**/__mocks__/",
    "!dist/**/*.spec.*",
    "support/"
  ],
  "keywords": [
    "aem design",
    "aem",
    "design",
    "webpack-configuration",
    "webpack",
    "compose",
    "node",
    "adobe",
    "webpack-dev-server",
    "front end",
    "front-end",
    "frontend",
    "live-reload",
    "watch",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "esmodule": "dist/index.es.js",
  "types": "dist/types",
  "sideEffects": false,
  "name": "@aem-design/compose-webpack",
  "homepage": "https://webpack.aem.design",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-design/npm-compose-webpack"
  },
  "bugs": {
    "url": "https://github.com/aem-design/npm-compose-webpack/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run build.compile.main && npm run build.compile.cli",
    "build.compile.main": "cross-env NODE_ENV=production microbundle build --format modern,es,cjs --no-sourcemap --target node --no-compress --tsconfig tsconfig.json --entry src/index.ts --output dist/index.js",
    "build.compile.cli": "cross-env NODE_ENV=production microbundle build --format cjs --no-sourcemap --target node --no-compress --tsconfig tsconfig.json --entry src/bin/cli-service.ts --output dist/bin/cli-service.js",
    "clean": "rm -rf dist",
    "dev": "npm run clean && npm run dev.watch",
    "dev.watch": "npm run clean && cross-env NODE_ENV=development tsc --watch --declaration true --declarationDir dist/types",
    "lint": "eslint '**/*.ts'",
    "postbuild": "chmod +x dist/bin/cli-service.js",
    "postversion": "npm run test && git push && git push --tags",
    "prepack": "npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "cross-env NODE_ENV=test jest --collectCoverageFrom=\"src/**/*.ts\" --coverage"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "version": "3.0.0-beta.0",
  "dependencies": {
    "@aem-design/compose-support": "1.2.1",
    "@babel/code-frame": "^7.10.4",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@babel/runtime-corejs3": "^7.11.2",
    "@types/babel__code-frame": "^7.0.2",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/figlet": "^1.2.0",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.11.2",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/sass": "^1.16.0",
    "@types/stylelint-webpack-plugin": "^2.1.0",
    "@types/terser-webpack-plugin": "^4.2.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-config-utils": "^2.3.1",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-env": "^1.15.3",
    "@types/webpack-merge": "^4.1.5",
    "@types/xml2js": "^0.4.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "chalk": "^4.1.0",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-webpack-plugin": "^2.1.0",
    "exports-loader": "^1.1.0",
    "extract-loader": "^5.1.0",
    "figlet": "^1.5.0",
    "file-loader": "^6.1.0",
    "imports-loader": "^1.1.0",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-console": "^1.0.1",
    "jest-mock-process": "^1.4.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^0.11.2",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.2",
    "stylelint-webpack-plugin": "^2.1.0",
    "terser-webpack-plugin": "^4.2.2",
    "ts-jest": "^26.4.1",
    "webpack": "5.0.0-rc.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-format-messages": "^2.0.6",
    "webpack-merge": "^5.1.4",
    "xml2js": "^0.4.23",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@chrisblossom/eslint-config": "^7.2.2",
    "@types/yargs": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "microbundle": "^0.12.4",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "eslint": "*",
    "stylelint": "*",
    "webpack": "*"
  }
}

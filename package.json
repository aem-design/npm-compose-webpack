{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "Webpack configuration and helpers for the AEM.Design compose module",
  "engines": {
    "node": ">= 10.x",
    "yarn": ">= 1.10.x"
  },
  "bin": {
    "aemdesign-compose": "dist/commonjs/bin/cli-service.js",
    "aemdesign-compose-modern": "dist/module/bin/cli-service.js"
  },
  "files": [
    "configs/",
    "dist/",
    ".browserlistrc"
  ],
  "keywords": [
    "aem design",
    "aem",
    "design",
    "webpack-configuration",
    "webpack",
    "compose",
    "node",
    "adobe",
    "webpack-dev-server",
    "front end",
    "front-end",
    "frontend",
    "live-reload",
    "watch",
    "typescript",
    "vue"
  ],
  "license": "Apache-2.0",
  "main": "dist/commonjs",
  "module": "dist/module",
  "types": "dist/types",
  "sideEffects": false,
  "name": "@aem-design/compose-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-design/npm-compose-webpack"
  },
  "bugs": {
    "url": "https://github.com/aem-design/npm-compose-webpack/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run build.module && npm run build.commonjs && npm run build.types",
    "build.module": "tsc --module es2015 --outDir dist/module --declaration false",
    "build.commonjs": "tsc --module commonjs --outDir dist/commonjs --declaration false",
    "build.types": "tsc --emitDeclarationOnly --declaration true --declarationDir dist/types",
    "clean": "rm -rf dist",
    "lint": "eslint '**/*.ts'",
    "postbuild": "chmod +x dist/commonjs/bin/cli-service.js",
    "postversion": "npm run test && git push && git push --tags",
    "prepack": "npm run build",
    "watch": "tsc --watch"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "version": "1.5.0",
  "dependencies": {
    "@aem-design/compose-support": "1.1.0",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/figlet": "^1.2.0",
    "@types/lodash": "^4.14.149",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^13.7.7",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/sass": "^1.16.0",
    "@types/stylelint-webpack-plugin": "^0.1.5",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "4.41.7",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-config-utils": "^2.3.0",
    "@types/webpack-dev-server": "^3.10.0",
    "@types/webpack-env": "^1.15.1",
    "@types/xml2js": "^0.4.5",
    "@vue/cli-service": "^4.2.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "chalk": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint-loader": "^3.0.3",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "extract-loader": "^4.0.3",
    "figlet": "^1.3.0",
    "file-loader": "^5.1.0",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.0",
    "stylelint-webpack-plugin": "^1.2.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.10.3",
    "webpack-format-messages": "^2.0.6",
    "xml2js": "^0.4.23",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "cross-env": "^7.0.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "eslint": "^6.8.0",
    "stylelint": "^13.2.0",
    "webpack": "^4.42.0"
  }
}

    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.9.0",
    "webpack-format-messages": "^2.0.5",
    "xml2js": "^0.4.22",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "husky": "^3.0.9",
    "typescript": "^3.7.2"
  }
}

{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "Webpack configuration and helpers for the AEM.Design compose module",
  "engines": {
    "node": ">= 12",
    "yarn": ">= 1.10.x < 2.x"
  },
  "bin": {
    "aemdesign-compose": "dist/bin/cli-service.js"
  },
  "files": [
    "configs/",
    "dist/",
    "!dist/**/__mocks__/",
    "!dist/**/*.spec.*",
    "support/"
  ],
  "keywords": [
    "aem design",
    "aem",
    "design",
    "webpack-configuration",
    "webpack",
    "compose",
    "node",
    "adobe",
    "webpack-dev-server",
    "front end",
    "front-end",
    "frontend",
    "live-reload",
    "watch",
    "typescript"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/types",
  "sideEffects": false,
  "name": "@aem-design/compose-webpack",
  "homepage": "https://webpack.aem.design",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-design/npm-compose-webpack"
  },
  "bugs": {
    "url": "https://github.com/aem-design/npm-compose-webpack/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run build.compile && npm run build.types",
    "build.compile": "cross-env NODE_ENV=production ttsc --declaration false --project tsconfig.es5.json",
    "build.types": "cross-env NODE_ENV=production ttsc --emitDeclarationOnly --declaration true --declarationDir dist/types --project tsconfig.es5.json",
    "clean": "rm -rf dist",
    "dev": "npm run clean && npm run dev.watch",
    "dev.watch": "cross-env NODE_ENV=development ttsc --watch --declaration true --declarationDir dist/types --project tsconfig.es5.json",
    "lint": "eslint --ext ts src",
    "postbuild": "chmod +x dist/bin/cli-service.js",
    "postversion": "npm run test && git push && git push --tags",
    "prepack": "npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "version": "3.0.0-beta.12",
  "dependencies": {
    "@aem-design/compose-support": "1.2.1",
    "@babel/code-frame": "^7.10.4",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "babel-loader": "^8.2.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "chalk": "^4.1.0",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "eslint-webpack-plugin": "2.3.0",
    "exports-loader": "^1.1.1",
    "extract-loader": "^5.1.0",
    "figlet": "^1.5.0",
    "file-loader": "^6.2.0",
    "imports-loader": "^1.2.0",
    "lodash": "^4.17.20",
    "mini-css-extract-plugin": "^1.3.1",
    "postcss": "^7.0.35",
    "postcss-loader": "^4.0.4",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-webpack-plugin": "^2.1.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.11.0",
    "webpack-format-messages": "^2.0.6",
    "webpack-merge": "^5.4.0",
    "xml2js": "^0.4.23",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@chrisblossom/eslint-config": "^7.2.2",
    "@types/babel__code-frame": "^7.0.2",
    "@types/copy-webpack-plugin": "^6.2.0",
    "@types/figlet": "^1.2.1",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/mini-css-extract-plugin": "^1.2.1",
    "@types/mock-fs": "^4.13.0",
    "@types/node": "^14.14.7",
    "@types/normalize-path": "^3.0.0",
    "@types/pify": "^3.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/sass": "^1.16.0",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack-bundle-analyzer": "^3.9.0",
    "@types/webpack-config-utils": "^2.3.1",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-env": "^1.15.3",
    "@types/webpack-merge": "^4.1.5",
    "@types/xml2js": "^0.4.6",
    "@types/yargs": "^15.0.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-mock-console": "^1.0.1",
    "jest-mock-process": "^1.4.0",
    "memfs": "^3.2.0",
    "mock-fs": "^4.13.0",
    "normalize-path": "^3.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.0.5",
    "typescript-transform-paths": "^2.0.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.11.0",
    "eslint": "^6.0.0 || ^7.0.0",
    "postcss": "^7.0.0",
    "stylelint": "^13.0.0",
    "webpack": "^5.0.0"
  }
}

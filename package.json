    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "extract-loader": "^3.1.0",
    "figlet": "^1.2.4",
    "file-loader": "^4.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "imports-loader": "^0.8.0",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",

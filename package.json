{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "Webpack configuration and helpers for the AEM.Design compose module",
  "engines": {
    "node": ">= 10.x"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "aem",
    "design",
    "webpack",
    "compose",
    "node"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "dist/*.d.ts",
  "name": "@aem-design/compose-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-design/npm-compose-webpack"
  },
  "bugs": {
    "url": "https://github.com/aem-design/npm-compose-webpack/issues"
  },
  "scripts": {
    "build": "npm run build.source && npm run build.types",
    "build.source": "cross-env NODE_ENV=production tsc",
    "build.types": "cross-env NODE_ENV=production tsc --project tsconfig.types.json",
    "lint": "eslint '**/*.ts'",
    "postversion": "npm run test && git push && git push --tags",
    "watch": "$(npm bin)/tsc --watch"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "version": "1.2.0",
  "dependencies": {
    "@aem-design/compose-support": "1.0.1",
